Question 7
using System;
  

namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(1);
            Console.WriteLine(101);
            Console.WriteLine(1001);
            Console.ReadLine();  
        }
    }
}    
