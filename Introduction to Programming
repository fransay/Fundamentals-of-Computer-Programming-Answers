 Question 8
 using System;
  

namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            DateTime now = DateTime.Now;
            Console.WriteLine(now);
            Console.ReadLine();  
        }
    }
}    

 
