namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            float num1 = 34.567839023f;
            double num2 = 5;
            float num4 = -5.01f;
            double num5 = 12.345;
   
   decimal num6 = 8923.1234857M;
            decimal num3 = 3456.091124875956542151256683467M;
            Console.ReadLine();  
        }
    }
} 


Question 3

using System;
  
namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter your numbers");
            float a = float.Parse(Console.ReadLine());
            float b = float.Parse(Console.ReadLine());

            bool equal = (a - b) < 0.000001;
            Console.WriteLine(equal);
            Console.ReadLine();  
        }
    }
}    
 
 
 
