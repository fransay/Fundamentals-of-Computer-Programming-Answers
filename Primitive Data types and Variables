  Question 13
  using System;
  
namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            int firstValue = 5;
            int secondValue = 10;
            
            
            Console.WriteLine("The first Value is {1} and the se" +
                "cond value is  {0}", firstValue, secondValue);

            Console.ReadLine();  
        }
    }
}    
 
 
