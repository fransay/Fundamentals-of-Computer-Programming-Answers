 Question 7
 
 using System;
  
namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            string hello = "Hello";
            string world = "World";
            object valueOfConcatenation = hello + " " + world;
            Console.WriteLine(valueOfConcatenation);
            Console.ReadLine();  
        }
    }
}    


 Question 8
 using System;
  
namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            string hello = "Hello";
            string world = "World";
            object valueOfConcatenation = hello + " " + world;
            string helloWorld;
            Console.WriteLine(valueOfConcatenation);
            Console.ReadLine();  
        }
    }
}   


 Question 9
 
 using System;
  
namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            string firstSentence = "The \"use\" of quotations causes difficulties";
            string secondSentence = "The use of quotations causes difficulties";
            Console.WriteLine(firstSentence);
            Console.WriteLine(secondSentence);
            Console.ReadLine();  
        }
    }
}    
 


 
 Question 10
 using System;
  
namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine( "oo                   oo");
            Console.WriteLine(" o                   o");
            Console.WriteLine("  o       o          o");
            Console.WriteLine("   o               o");
            Console.WriteLine("    o            o");
            Console.WriteLine("     o         o");
            Console.WriteLine("      o      o");
            Console.WriteLine("       o   o");
            Console.WriteLine("         o");
// Design of the shape of the heart is relative. LoL you should make yours a better one

            Console.ReadLine();  
        }
    }
}    
 
 
