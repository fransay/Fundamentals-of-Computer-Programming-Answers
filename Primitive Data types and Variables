Question 1
// Note: solution to this question varies  
 
using System;

namespace IntroductionToProgramming
{
    class Program
    {
        static void Main(string[] args)
        {
            
            int number1 = 52130;
            sbyte number2 = -115;
            long number3 = 4825932;
            byte number4 = 97;
            int number5 = -10000;
            int number6 = 20000;
            decimal number7 = 970700000M;
            byte number8 = 112;
            sbyte number9 = -44;
            long number10 = 1000000;
            int number11 = 1990;
            decimal number12 = 123456789123456789M;
            Console.ReadLine();  
        }
    }
}    


Question 2 

